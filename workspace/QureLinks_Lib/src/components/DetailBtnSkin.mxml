<?xml version="1.0" encoding="utf-8"?>
<s:Skin xmlns:s="library://ns.adobe.com/flex/spark"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		width="83" height="38">
	<fx:Metadata>[HostComponent("spark.components.Button")]</fx:Metadata>
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>
	<s:BitmapImage includeIn="down,up,disabled" x="0" y="0" smooth="true"
				   source="@Embed('/assets/images/user/ng_button.png')"/>
	<s:BitmapImage includeIn="over" x="0" y="0" smooth="true"
				   source="@Embed('/assets/images/user/ng_button-over.png')"/>
</s:Skin>
